  "scripts": {
    "setup": "npm install",
    "postsetup": "lerna bootstrap",
    "clean": "lerna clean && rimraf ./node_modules",
    "build": "lerna run build --ignore xebra*",
    "build-release": "lerna run build-release --ignore xebra*",
    "serve": "lerna run serve --ignore xebra* --stream",
    "lint": "eslint src -c ./.eslintrc.json",
    "lint-fix": "eslint src -c ./.eslintrc.json --fix",
    "test": "npm run lint",
    "testall": "npm run test && lerna run test",
    "publish": "node ./.bin/bumpMWVersion.js; lerna run test; lerna publish"
