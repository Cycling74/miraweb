	"scripts": {
		"build": "rm -rf ./dev_build && webpack --config webpack/dev.config.js",
		"build-release": "rm -rf ./build && webpack --config webpack/release.config.js",
		"watch": "rm -rf ./dev_build && env WATCH=1 webpack --config webpack/dev.config.js",
		"postupdate": "cd node_modules/pixi.js && npm install && npm run lib",
		"postinstall": "cd node_modules/pixi.js && npm install && npm run lib",
		"lint": "eslint src -c .eslintrc.json",
		"lint-fix": "eslint src -c .eslintrc.json --fix",
		"test": "npm run lint"
	},
