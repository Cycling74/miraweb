  "scripts": {
    "setup": "node ./.bin/setup.js",
    "setup:pixi": "node ./.bin/buildPixi.js",
    "clean": "node ./.bin/clean.js",
    "build": "rimraf ./dev_build && cross-env MW_DEV_SERVER=false webpack --config webpack/dev.config.js",
    "build-release": "rimraf ./build && webpack --config webpack/release.config.js",
    "serve": "cross-env MW_DEV_SERVER=true webpack-dev-server --config webpack/dev.config.js --open --inline --progress",
    "lint": "eslint src -c ./.eslintrc.json",
    "lint-fix": "eslint src -c ./.eslintrc.json --fix",
    "test": "npm run lint",
    "testall": "npm run test && lerna run test",
    "publish": "node ./.bin/bumpMWVersion.js; lerna run test; lerna publish"
  },
